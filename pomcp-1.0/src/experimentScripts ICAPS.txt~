#Experiment 2e - Ad-hoc coordination
./pomcp --problem kitchen --comm1 true --comm2 false --randomcomm true --random1 true --outputfile output-kitchen-RandN.txt
./pomcp --problem kitchen --comm1 true --comm2 true --randomcomm true --random1 true --outputfile output-kitchen-RandComm.txt
./pomcp --problem kitchen --comm1 true --comm2 true --randomcomm true --random1 true --humandefined2 true --outputfile output-kitchen-RandH.txt
./pomcp --problem kitchen --comm1 true --comm2 true --randomcomm true --outputfile output-kitchen-CommComm.txt
./pomcp --problem kitchen --comm1 true --comm2 true --randomcomm true --humandefined1 true --humandefined2 true --outputfile output-kitchen-HH.txt

