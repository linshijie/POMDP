#Boxpushing
#Experiment 1a
./pomcp --problem boxpushing --comm1 false --comm2 false --outputfile output-box-NN.txt
./pomcp --problem boxpushing --comm1 true --comm2 true --messageloss 0.0 --messagedelay 0.0 --messagemisinterp 0.0 --outputfile output-box-CommComm-0.0-0.0-0.0.txt
./pomcp --problem boxpushing --comm1 true --comm2 true --messageloss 1.0 --messagedelay 1.0 --messagemisinterp 1.0 --outputfile output-box-CommComm-1.0-1.0-1.0.txt

#Kitchen
